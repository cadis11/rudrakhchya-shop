{
  "name": "rudrakhchya-shop",
  "private": true,
  "version": "0.1.0",
  "scripts": {
  "dev": "next dev",
  "build": "prisma generate && next build",
  "start": "next start",
  "lint": "next lint",
  "typecheck": "tsc --noEmit",
  "prisma:generate": "prisma generate",
  "prisma:migrate": "prisma migrate dev --name init",
  "seed": "tsx scripts/seed.ts",
  "test": "playwright test",
  "cms:studio": "sanity dev --cwd cms",
  "cms:build": "sanity build --cwd cms",
  "cms:import": "sanity dataset import cms/seed.ndjson $SANITY_DATASET --replace",
  "dev:ngrok": "ngrok http 3000",
  "postinstall": "prisma generate"
},
  "dependencies": {
    "@aws-sdk/client-s3": "3.676.0",
    "@aws-sdk/s3-request-presigner": "3.676.0",
    "@prisma/client": "5.19.0",
    "@sanity/client": "6.21.2",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "clsx": "2.1.1",
    "groq": "3.60.0",
    "next": "^14.2.32",
    "next-auth": "^5.0.0-beta.29",
    "next-intl": "3.23.1",
    "nodemailer": "6.9.14",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "2.5.2",
    "zod": "3.23.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "1.47.2",
    "@types/node": "20.14.15",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.6",
    "postcss": "8.4.47",
    "prisma": "5.19.0",
    "sanity": "^4.5.0",
    "tailwindcss": "3.4.10",
    "tsx": "^4.20.5",
    "typescript": "5.5.4"
  }
}
